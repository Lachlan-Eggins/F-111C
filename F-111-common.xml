	 <input>
        <!--		<keyboard n="0" include="/Input/Keyboard/carrier-bindings.xml"> -->
        <keyboard n="0" >
            <multikey include="Systems/instruments-multikey.xml"/>

            <key n="1">
                <name>CTRL-a</name>
                <desc>Toggle autopilot altitude mode.</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>sim/model/f15/controls/AFCS/alt-hold</property>
                </binding>
            </key>

            <key n="4">
                <name>CTRL-d</name>
                <desc>Radar mode.</desc>
                <binding>
                    <command>nasal</command>
                    <script>awg_9.radar_mode_cycle()</script>
                </binding>
            </key>

            <key n="8">
                <name>CTRL-h</name>
                <desc>Toggle autopilot heading mode.</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.afcs_heading_switch(0);</script>
                </binding>
            </key>

            <key n="14">
                <name>Ctrl-n</name>
                <desc>Toggle Radar PULSE SRCH / TWS AUTO Modes</desc>
                <binding>
                    <command>nasal</command>
                    <script>awg_9.wcs_mode_toggle()</script>
                </binding>
            </key>

            <key n="19">
                <name>Ctrl-s</name>
                <desc>Toggle Auto Throttle hold</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.toggleAutoThrottle();</script>
                </binding>
            </key>

            <key n="20">
                <name>Ctrl-t</name>
                <desc>Toggle autopilot attitude mode</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>sim/model/f15/controls/AFCS/att-hold</property>
                </binding>
            </key>

            <key n="23">
                <name>Ctrl-w</name>
                <desc>Cycle through MasterArm modes</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.master_arm_cycle();</script>
                </binding>
            </key>

            <key n="42">
                <name>*</name>
                <desc>Nose wheel steering maneuvering toggle</desc>
                <binding>
                    <command>nasal</command>
                    <script type="string">
                        <![CDATA[ 
                            if (getprop("fdm/jsbsim/fcs/steer-maneuver"))
                            {
                                gui.popupTip("Maneuver NWS disabled");
                                setprop("fdm/jsbsim/fcs/steer-maneuver",0)
                            }
                            else
                            {
                                gui.popupTip("Maneuver NWS enabled");
                                setprop("fdm/jsbsim/fcs/steer-maneuver",1)
                            }
                    ]]>
                    </script>
                </binding>
            </key>


                      <!-- map keys for catapult launch -->
            <key n="67">
                <name>C</name>
                <desc>Catapult Launch Command.</desc>
                <binding>
                    <command>nasal</command>
                    <script>
                        if (getprop("fdm/jsbsim/systems/holdback/holdback-cmd"))
                        {
                        print("Catapult launch");
                        setprop("fdm/jsbsim/systems/catapult/cat-launch-cmd",1);
                        setprop("fdm/jsbsim/systems/holdback/holdback-cmd",0);
                        }
                        else
                        {
                        }
                    </script>
                </binding>
            </key>
                <key n="69">
                <name>E</name>
                <desc>Decrease Radar Range</desc>
                <binding>
                    <command>nasal</command>
                    <script>awg_9.range_control(-1);</script>
                </binding>
            </key>

            <key n="70">
                <name>F</name>
                <desc>Raise flaps</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.raiseFlaps();</script>
                </binding>
            </key>

			<!-- map keys for launch-bar -->
            <key n="76">
                <name>L</name>
                <desc>Launch-bar engage.</desc>
                <binding>
                    <command>nasal</command>
                    <script>
                        if (getprop("fdm/jsbsim/systems/holdback/holdback-cmd"))
                        {
                        print("Disengage launchbar");
                        setprop("fdm/jsbsim/systems/catapult/cat-launch-cmd",0);
                        setprop("fdm/jsbsim/systems/holdback/holdback-cmd",0);
                        setprop("controls/gear/brake-parking",1);
                        }
                        else
                        {
                        print("Engage launchbar");
                        setprop("fdm/jsbsim/systems/catapult/cat-launch-cmd",0);
                        setprop("fdm/jsbsim/systems/holdback/holdback-cmd",1);
                        setprop("controls/gear/brake-parking",0);
                        }
                    </script>
                </binding>
            </key>
            <key n="81">
                <name>Q</name>
                <desc>Reset view</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.resetView();</script>
                </binding>
            </key>
            <key n="82">
                <name>R</name>
                <desc>Increase Radar Range</desc>
                <binding>
                    <command>nasal</command>
                    <script>awg_9.range_control(1);</script>
                </binding>
            </key>

            <key n="99">
                <name>c</name>
                <desc>Open canopy</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>fdm/jsbsim/systems/ecs/canopy-position-dmd</property>
                </binding>
            </key>

            <key n="101">
                <name>e</name>
                <desc>Trigger M61A1 Vulcan or AIM-9s</desc>
                <binding>
                    <command>property-assign</command>
                    <property>controls/armament/trigger</property>
                    <value>true</value>
                </binding>
                <mod-up>
                    <binding>
                        <command>property-assign</command>
                        <property>controls/armament/trigger</property>
                        <value>false</value>
                    </binding>
                </mod-up>
            </key>

            <key n="104">
                <name>h</name>
                <desc>HUD Cycle</desc>
                <binding>
                    <command>nasal</command>
                    <script type="string">
                        <![CDATA[ 
                        if(getprop("sim/hud/visibility[1]"))
                        {
                            setprop("sim/hud/visibility[0]",0);
                            setprop("sim/hud/visibility[1]",0);
                        }
                        else
                        {
                            setprop("sim/hud/visibility[0]",1);
                            setprop("sim/hud/visibility[1]",1);
                        }
                        ]]>
                    </script>
                </binding>
            </key>

            <key n="111">
                <name>o</name>
                <desc>Toggle arrester hook</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>fdm/jsbsim/systems/hook/tailhook-cmd-norm</property>
                </binding>
            </key>
            <key n="113">
                <name>q</name>
                <desc>Toggle radar standby mode</desc>
                <binding>
                    <command>nasal</command>
                    <script>awg_9.toggle_radar_standby();</script>
                </binding>
            </key>

            <key n="17">
                <name>ctrl-q</name>
                <desc>Trigger flare release</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>/ai/submodels/submodel[5]/flare-release-cmd</property>
                    <value type="bool">true</value>
                </binding>
            </key>

 
            <key n="109">
                <name>m</name>
                <desc>Cycle Weapon Mode Selector</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>sim/model/f15/controls/armament/weapon-selector</property>
                    <step>1</step>
                    <min>0</min>
                    <max>3</max>
                    <repeat>false</repeat>
                    <wrap>true</wrap>
                </binding>
            </key>
            <key n="121">
                <name>y</name>
                <desc>Next target select</desc>
                <binding>
                    <command>property-assign</command>
                    <property>sim/model/f15/instrumentation/radar-awg-9/select-target</property>
                    <value>1</value>
                </binding>
            </key>

            <key n="89">
                <name>Y</name>
                <desc>Previous target select</desc>
                <binding>
                    <command>property-assign</command>
                    <property>sim/model/f15/instrumentation/radar-awg-9/select-target</property>
                    <value>-1</value>
                </binding>
            </key>

            <key n="362">
                <name>Home</name>
                <desc>Increase elevator trim</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.trimUp();</script>
                </binding>
            </key>

            <key n="363">
                <name>End</name>
                <desc>Decrease elevator trim</desc>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.trimDown();</script>
                </binding>
            </key>

            <key n="21">
              <name>Ctrl-U</name>
              <desc>[Cheat] Add 1000ft of emergency altitude</desc>
              <binding>
               <command>property-adjust</command>
               <property>/position/altitude-ft</property>
               <step type="double">1000.0</step>
              </binding>
              <binding>
               <command>property-assign</command>
               <property>/sim/startup/onground</property>
               <value type="bool">false</value>
              </binding>
              <binding>
               <command>nasal</command>
               <script>aircraft.ct("c-u")</script>
              </binding>
             </key>

             <key n="97">
              <name>a</name>
              <desc>Increase speed-up</desc>
                <binding>
                    <command>nasal</command>
                    <script>controls.speedup(1);</script>
                </binding>
                <binding>
               <command>nasal</command>
               <script>aircraft.ct("a")</script>
              </binding>
             </key>

             <key n="65">
              <name>A</name>
              <desc>Decrease speed-up</desc>
              <binding>
                <command>nasal</command>
                <script>controls.speedup(-1);</script>
              </binding>
              <binding>
               <command>nasal</command>
               <script>aircraft.ct("a")</script>
              </binding>
             </key>
        </keyboard>
