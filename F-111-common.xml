                    <count type="int">2</count>
                </signals>
                <signals include="/Aircraft/Generic/flightrecorder/components/faults-engines.xml">
                    <count type="int">2</count>
                </signals>
                <signals include="/Aircraft/Generic/flightrecorder/components/tanks.xml">
                    <count type="int">12</count>
                </signals>
                <signals include="/Aircraft/Generic/flightrecorder/components/gear-retractable.xml">
                    <count type="int">3</count>
                </signals>
			</config>
		</flight-recorder>
		
		<help>
            <title>General Dynamics F-111 Aardvark</title>
            <key>
                <name>Home</name>
                <desc>Increase elevator trim</desc>
            </key>
            <key>
                <name>End</name>
                <desc>Decrease elevator trim</desc>
            </key>
            <key>
                <name>E</name>
                <desc>Decrease Radar Range</desc>
            </key>
            <key>
                <name>o</name>
                <desc>toggle arrester hook</desc>
            </key>
            <key>
                <name>R</name>
                <desc>Increase Radar Range</desc>
            </key>
            <key>
                <name>c</name>
                <desc>Open/Close canopy</desc>
            </key>

            <line>__________________________________________________</line>

            
            <key>
                <name>q</name>
                <desc>Toggle Radar Standby Mode</desc>
            </key>
            <key>
                <name>m</name>
                <desc>Cycle Stick Weapon Mode Selector</desc>
            </key>
            <key>
                <name>Ctrl-w</name>
                <desc>Cycle through Master Arm modes</desc>
            </key>
            <key>
                <name>e</name>
                <desc>Fire guns / missiles</desc>
            </key>
            <key>
                <name>y/Y</name>
                <desc>Select next/prev target (by distance).</desc>
            </key>
            <key>
                <name>Ctrl-q</name>
                <desc>Release chaff/flare</desc>
                <line>__________________________________________________</line>
			</key>
        </help>
		
