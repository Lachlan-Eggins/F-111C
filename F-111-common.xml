                        </binding>
                    </item>
                    <item n="2">
                        <label>Liveries</label>
                        <binding>
                            <command>nasal</command>
                            <script>aircraft.livery.dialog.toggle()</script>
                        </binding>
                    </item>
                    <!--
					<item n="4">
						<label>Select MP back seater</label>
